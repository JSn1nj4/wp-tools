{
    "name": "jsn1nj4/wp-tools",
    "description": "Simple helper tools for working with WordPress and WordPress-specific data.",
    "keywords": [
        "framework",
        "laravel",
        "laravel zero",
        "console",
        "cli"
    ],
    "homepage": "https://github.com/JSn1nj4/wp-tools",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/JSn1nj4/wp-tools/issues",
        "source": "https://github.com/JSn1nj4/wp-tools"
    },
    "require": {
        "php": "^8.0",
        "laravel-zero/framework": "^8.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "wp-tools"
    ]
}
